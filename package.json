{
  "name": "crowds-play",
  "version": "1.3.3",
  "private": true,
  "scripts": {
    "lint": "standard",
    "dev": "next",
    "clean": "rm -rf .next bundles",
    "analyze": "BUNDLE_ANALYZE=both next",
    "analyze-server": "BUNEL_ANALYZE=server next",
    "build": "next build",
    "start": "NODE_ENV=production next start",
    "predeploy": "npm run build",
    "deploy": "now --public",
    "relock": "rm -rf node_modules package-lock.json && npm install",
    "preexport": "npm run clean && npm run build",
    "export": "NODE_ENV=dev next export -o ../App/www",
    "export-prod": "npm run export -o ../App/www",
    "postexport": "find ../App/www -type f \\( -iname \\*.js -o -iname \\*.map -o -iname \\*.css -o -iname \\*.html \\) -print0 | xargs -0 sed -i '' -e 's/\"\\/_next/\"_next/g' # Recursively replace `\"/_href` with `\"_href` in produced code",
    "clean-native": "rm -rf ../App/www/*",
    "jsinspect": "jsinspect --ignore .next"
  },
  "dependencies": {
    "accept-language-parser": "1.5.0",
    "autoprefixer": "9.5.1",
    "babel-plugin-transform-define": "1.3.1",
    "babel-polyfill": "6.26.0",
    "btoa": "1.2.1",
    "clipboard": "2.0.4",
    "core-js": "3.0.1",
    "deep-equal": "1.0.1",
    "deep-object-diff": "1.1.0",
    "fuzzaldrin": "2.1.0",
    "ip": "1.1.5",
    "isomorphic-unfetch": "3.0.0",
    "lodash.clonedeep": "4.5.0",
    "lodash.debounce": "4.0.8",
    "lodash.get": "4.4.2",
    "lodash.isequal": "4.5.0",
    "lodash.merge": "4.6.1",
    "lodash.orderby": "4.6.0",
    "lodash.pull": "4.1.0",
    "lodash.pullat": "4.6.0",
    "lodash.trim": "4.5.1",
    "lodash.unescape": "4.0.1",
    "moment": "2.24.0",
    "next": "8.0.4",
    "next-compose-plugins": "2.1.1",
    "next-manifest": "1.6.0",
    "next-offline": "3.3.8",
    "postcss-nested": "4.1.2",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-beautiful-dnd": "11.0.0",
    "react-calendar-heatmap": "1.8.0",
    "react-dom": "16.8.6",
    "react-player": "1.11.0",
    "redux": "4.0.1",
    "redux-devtools-extension": "2.13.8",
    "redux-localstorage": "0.4.1",
    "redux-sessionstorage": "0.4.0",
    "redux-thunk": "2.3.0",
    "socket.io": "2.2.0",
    "socket.io-client": "2.2.0",
    "styled-jsx-plugin-postcss": "2.0.0"
  },
  "devDependencies": {
    "jsinspect": "0.12.7",
    "standard": "12.0.1",
    "webpack-bundle-analyzer": "3.3.0",
    "@zeit/next-bundle-analyzer": "0.1.2"
  },
  "homepage": "https://crowds-play.com",
  "engines": {
    "node": "8.10.0"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ]
}
